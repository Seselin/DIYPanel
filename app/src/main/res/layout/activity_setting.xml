<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_bg"
    android:orientation="vertical">

    <include layout="@layout/view_head" />

    <TextView
        android:id="@+id/tv_set_num"
        style="@style/clickItemStyle"
        android:layout_marginTop="1dp"
        android:text="抽奖格子数设定" />

    <TextView
        android:id="@+id/tv_select_combination"
        style="@style/clickItemStyle"
        android:layout_marginTop="1dp"
        android:text="奖品组合选择" />

    <TextView
        android:id="@+id/tv_edit_combination"
        style="@style/clickItemStyle"
        android:layout_marginTop="1dp"
        android:text="奖品组合编辑" />

    <TextView
        android:id="@+id/tv_edit_contain"
        style="@style/clickItemStyle"
        android:layout_marginTop="1dp"
        android:text="奖品库编辑" />

    <TextView
        android:id="@+id/tv_clear_contain"
        style="@style/clickItemStyle"
        android:layout_marginTop="1dp"
        android:text="清空奖品库" />

    <TextView
        android:id="@+id/tv_set_default_contain"
        style="@style/clickItemStyle"
        android:layout_marginTop="1dp"
        android:text="初始化奖品库" />


</LinearLayout>